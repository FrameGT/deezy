<title>Deezy</title>
<meta name=description content='DHCP configuration application for The Gevirtz School'>
<meta name=author content='Justin Force <justin.force@gmail.com>'>
<meta charset=utf-8>
<%= stylesheet_link_tag 'application' %>
<% if RAILS_ENV == 'production' %>
  <%= javascript_include_tag 'all_min' %>
<% else %>
  <%= javascript_include_tag 'prototype','effects','application' %>
<% end %>
<div id=faco>
  <div id=facr>
    <div id=facc>
      <header>
      <h1><a href=/><%= image_tag 'deezy.png' %></a></h1>
      <ul>
        <li><%= link_to 'Show All', '/' %>
        <li><%= link_to 'New Entry', new_entry_path %>
        <li><%= link_to 'dhcpd.conf', '/dhcpd.conf' %>
        <li><%= link_to 'dhcpd.leases', '/dhcpd.leases' %>
        <li>

        <% form_for @search do |f| %>
          <%= f.text_field :any_like %>
        <% end %>
      </ul>

      </header>
      <section id=content style=display:block>
      <%= yield %>
      </section>
